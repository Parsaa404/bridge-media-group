generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SectionContent {
  id          Int      @id @default(autoincrement())
  section     String   @unique
  description String   @db.Text
  features    String   @db.Text
  imageUrl    String
}
